{
  "name": "tradeware-ui",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "npx next dev",
    "dev:backend": "cd backend && npm run dev",
    "dev:all": "concurrently \"npm run dev:backend\" \"npm run dev\" --names \"backend,frontend\" --prefix-colors \"blue,green\"",
    "build": "prisma generate && npx next build",
    "start": "npx next start",
    "lint": "npx next lint",
    "clean": "node -e \"try{require('fs').rmSync('.next',{recursive:true,force:true});console.log('Cleaned .next')}catch(e){}\"",
    "build:clean": "npm run clean && npx next build",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "postinstall": "prisma generate",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@react-three/drei": "^9.122.0",
    "@react-three/fiber": "^8.18.0",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.9.0",
    "clsx": "^2.1.0",
    "framer-motion": "^10.18.0",
    "gsap": "^3.14.2",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.344.0",
    "mongoose": "^8.22.0",
    "next": "14.2.18",
    "next-themes": "^0.4.6",
    "nodemailer": "^8.0.1",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "socket.io-client": "^4.8.3",
    "three": "^0.161.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.18",
    "postcss": "^8.4.33",
    "prisma": "^5.22.0",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "overrides": {
    "react": "^18.3.0",
    "react-dom": "^18.3.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
